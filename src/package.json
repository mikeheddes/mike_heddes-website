{
  "name": "@mikeheddes-website/src",
  "author": "Mike Heddes",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint './**/*.js?(x)'",
    "lint:js": "eslint --fix './**/*.js?(x)'",
    "build:client": "NODE_ENV=development webpack --config ./webpack/client.config.js",
    "build:server": "NODE_ENV=development webpack --config ./webpack/server.config.js",
    "build:client:prod": "NODE_ENV=production webpack --config ./webpack/client.config.js",
    "build:server:prod": "NODE_ENV=production webpack --config ./webpack/server.config.js",
    "build": "yarn build:server && yarn build:client",
    "build:prod": "yarn build:server:prod && yarn build:client:prod",
    "build:dll": "webpack --config ./webpack/dll.config.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@loadable/component": "^5.0.1",
    "@loadable/server": "^5.1.0",
    "@mdx-js/tag": "^0.16.0",
    "apollo-client": "^2.4.5",
    "connected-react-router": "^4.4.1",
    "delay": "^3.0.0",
    "graphql": "^14.0.2",
    "history": "^4.7.2",
    "polished": "^1.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.1",
    "react-helmet-async": "^0.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-spring": "^5.6.1",
    "redux": "^4.0.0",
    "reselect": "^3.0.1",
    "stackblur-canvas": "^1.4.1",
    "styled-components": "^4.0.3",
    "styled-components-breakpoint": "^2.1.0",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "@loadable/babel-plugin": "^5.0.1",
    "@loadable/webpack-plugin": "^5.0.2",
    "@mdx-js/loader": "^0.16.0",
    "@mdx-js/mdx": "^0.16.0"
  }
}
