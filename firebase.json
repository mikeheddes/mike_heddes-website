{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "hosting": {
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|webp|js)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        }
      ]
    }
  }
}
