{
  "name": "mikeheddes-website",
  "version": "1.0.0",
  "author": "Mike Heddes",
  "description": "My portfolio website",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "functions",
    "src"
  ],
  "repository": "https://github.com/mikeheddes/mikeheddes-website.git",
  "scripts": {
    "start": "cd ./functions && yarn start:dev",
    "start:prod": "cd ./functions && yarn start:prod",
    "lint": "cd ./functions && yarn lint && cd ../src && yarn lint",
    "build": "yarn build:prod",
    "build:prod": "cd ./src && yarn build:prod",
    "build:dev": "cd ./src && yarn build",
    "build:dll": "cd ./src && yarn build:dll",
    "clean:modules": "rm -rf ./functions/node_modules && rm -rf ./src/node_modules"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.7.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "imagemin-webpack-plugin": "^2.2.0",
    "prettier": "^1.14.2",
    "remark-highlight.js": "^5.0.0",
    "responsive-loader": "^1.1.0",
    "sharp": "^0.20.5",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-manifest-plugin": "^2.0.3"
  }
}