{
  "name": "mike-heddes-portfolio",
  "version": "0.0.1",
  "author": "Mike Heddes",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development && npm run build:dll && node server",
    "start:prod": "npm run clean:build && npm run build && cross-env NODE_ENV=production node server",
    "start:tunnel": "cross-env NODE_ENV=development node server --tunnel",
    "start:sync": "npm run build:dll && cross-env NODE_ENV=development node server --sync",
    "start:sync:tunnel": "npm run build:dll && cross-env NODE_ENV=development node server --sync --tunnel",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --hide-modules --color -p --progress --hide-modules --display-optimization-bailout",
    "build:dll": "node webpack/makeDlls.js",
    "clean": "npm run clean:build && npm run clean:dll",
    "clean:build": "rimraf ./build",
    "clean:dll": "rimraf ./dlls"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "browserslist": [
    "> 5%"
  ],
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "gsap": "^1.20.3",
    "highlight.js": "^9.12.0",
    "ip": "^1.1.5",
    "lodash": "^4.17.5",
    "marksy": "^6.0.3",
    "minimist": "^1.2.0",
    "opn": "^5.2.0",
    "polished": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-event-listener": "^0.5.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "1.x",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "shelljs": "^0.8.1",
    "styled-components": "^3.1.6",
    "whatwg-fetch": "^2.0.3"
  },
