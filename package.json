{
  "name": "test-hmr",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development && npm run build:dll && npm run server:dev",
    "start:prod": "npm run build:clean && npm run build && npm run server:prod",
    "server:dev": "cross-env NODE_ENV=development node server/index.js",
    "server:prod": "cross-env NODE_ENV=production node server/index.js",
    "build:dll": "node webpack/makeDlls.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --hide-modules --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build"
  },
  "babel": {
    "plugins": [
      "styled-components"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "ip": "^1.1.5",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "opn": "^5.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "shelljs": "^0.8.1",
    "styled-components": "^3.1.6",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "circular-dependency-plugin": "^4.4.0",
    "css-loader": "^0.28.9",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.7.0",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  }
}